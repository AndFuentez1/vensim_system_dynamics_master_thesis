		[ForecastYear,Zone]
	                < Min Grid Capacity
	         ,0
	         ,Max(0,
	            (Surplus by Zone[ForecastYear,Hour,Zone]+Grid Security Margin-Adjusted Interconnection Capacity\
		[ForecastYear,Zone])))
	)
	~	MWh
	~	IF THEN ELSE(Sum(Expected Interconnection \
		Capacity[ForecastYear,Int!]*Interconnection Pairs Cases[Int!,Zone])=0,
